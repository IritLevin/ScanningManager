<?xml version="1.0" encoding="utf-8" ?>
<configuration>
	<appSettings>
		<!-- access these values via the property:
			System.Configuration.ConfigurationManager.AppSettings[key]
		-->
		
		<!-- Scanners and Relay parameters-->
		<add key="ImagesFolder" value="D:\" /> <!--Default folder for scanned images-->
		<add key="DevconPath" value="devcon.exe" />

		<add key="ScannerNames" value="1,2" /> <!-- A list of connected scanners-->
		<add key="InstanceIDs" value="USB\VID_04A9&amp;PID_1907\5&amp;9345ECD&amp;0&amp;4,USB\VID_04A9&amp;PID_1907\5&amp;9345ECD&amp;0&amp;3"/> <!-- A list of corresponding instance ids -->
		<add key="ScannerPowerPorts" value="-1,-1,-1,0,-1,-1,1" /> <!--Relay port ID for each scanner (-1 if none)-->
		<add key="ScannerRelay" value="DEVCON,DEVCON,NONE,A06167,NONE,DEVCON,A06167" /> <!--Relay name or disconnection method-->
		<add key="EnvRoomExist" value="0" />	<!-- Save environmental data -->
		<add key="EnableDelay" value="1000" />	<!-- Time interval (in ms) between enabling scanner and scanning -->
		<add key="DisableDelay" value="1000" />	<!-- Time interval (in ms) between disabling scanner and continuing -->
		<add key="ScnrPropsNames" value="Horizontal Resolution,Vertical Resolution,Data Type,Horizontal Extent,Vertical Extent" /> <!-- An array of scanner properties that needs to be changed from default-->
		<add key="ScnrPropsVals" value="300,300,3,2550,3501" /> <!-- An array of properties values -->
		
		<!-- Email and SMS sender using standard SMTP-->
		<add key="GmailSenderUser" value="user@gmail.com" />
		<add key="GmailSenderPassword" value="userPassword" />
		<add key="ICQSenderUid" value="ICQID" />
		<add key="ICQSenderPassword" value="ICQPass" />
	
		<!--  Environmental Room  -->
		<add key="ReadValuesURI" value="/cgi-bin/ReadFile.exe?" />
        <add key="OrderValuesURI" value="/cgi-bin/OrderValues.exe?" />
        <add key="SetValuesURI" value="/cgi-bin/writeVal.exe?PDP%2C%2C" />
        <add key="ControllerIP" value="132.64.81.98" />
        <add key="DefaultRecordingInterval"  value="5" />
		<add key="ControlerIsReadonly" value="true" />
           
        <!--  Environmental Room: Parameter To read  -->
        <add key="ParamsID" value="1080,0003,1082,0002,1081,0001,0032" />
        <add key="ParamsName"  value="Pressure,PressureSP,Humidity,HumiditySP,Temperature,TemperatureSP,Steam" />
        <add key="ParamsLongName"  value="Pressure,Pressure Set Point,Humidity,Humidity Set Point,Temperature,Temperature Set Point,Steam Generator" />
        <add key="ParamsReadOnly"  value="1,0,1,0,1,0,1" />
	
	</appSettings>
	
</configuration>
