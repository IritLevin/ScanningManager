<?xml version="1.0" encoding="utf-8" ?>
<configuration>
	<appSettings>
		<!-- access these values via the property:
			System.Configuration.ConfigurationManager.AppSettings[key]
		-->
		
		<!-- Scanners and Relay parameters-->
		<add key="ImagesFolder" value="D:\" /> <!--Default folder for scanned images-->
		<add key="DevconPath" value="devcon.exe" />
		<add key="ScanningDPI" value="300" /> <!--Scanning Resolution-->
		<add key="ScannerNames" value="EPSON Perfection V200 #5,EPSON Perfection V200 #7,EPSON Perfection V200 #2,EPSON Perfection V200 #1,EPSON Perfection V200 #3" /> <!-- A list of connected scanners-->
		<add key="ScannerPowerPorts" value="-1,0,-1,-1,1" /> <!--Relay port ID for each scanner (-1 if none)-->
		<add key="ScannerRelay" value="NONE,A06167,NONE,DEVCON,A06167" /> <!--Relay name or disconnection method-->
		<add key="EnvRoomExist" value="1" />	<!-- Save environmental data -->
		
		<!-- Email and SMS sender using standard SMTP-->
		<add key="GmailSenderUser" value="user@gmail.com" />
		<add key="GmailSenderPassword" value="password" />
		<add key="SMSProviderEmailServer" value="@spikkosms.com" />
	
		<!--  Environmental Room  -->
		<add key="ReadValuesURI" value="/cgi-bin/ReadFile.exe?" />
        <add key="OrderValuesURI" value="/cgi-bin/OrderValues.exe?" />
        <add key="SetValuesURI" value="/cgi-bin/writeVal.exe?PDP%2C%2C" />
        <add key="ControllerIP" value="132.64.81.98" />
        <add key="DefaultRecordingInterval"  value="5" />
		<add key="ControlerIsReadonly" value="true" />
           
        <!--  Environmental Room: Parameter To read  -->
        <add key="ParamsID" value="1080,0003,1082,0002,1081,0001,0032" />
        <add key="ParamsName"  value="Pressure,PressureSP,Humidity,HumiditySP,Temperature,TemperatureSP,Steam" />
        <add key="ParamsLongName"  value="Pressure,Pressure Set Point,Humidity,Humidity Set Point,Temperature,Temperature Set Point,Steam Generator" />
        <add key="ParamsReadOnly"  value="1,0,1,0,1,0,1" />
	
	</appSettings>
	
</configuration>
